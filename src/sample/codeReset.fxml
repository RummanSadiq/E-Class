<GridPane alignment="center" hgap="10" stylesheets="@codesent.css" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">

    <columnConstraints>
        <ColumnConstraints maxWidth="273.2" minWidth="273.2" prefWidth="273.2" />
        <ColumnConstraints maxWidth="273.2" minWidth="273.2" prefWidth="273.2" />
        <ColumnConstraints maxWidth="273.2" minWidth="273.2" prefWidth="273.2" />
        <ColumnConstraints maxWidth="273.2" minWidth="273.2" prefWidth="273.2" />
        <ColumnConstraints maxWidth="273.2" minWidth="273.2" prefWidth="273.2" />
    </columnConstraints>

    <rowConstraints>
        <RowConstraints maxHeight="141" minHeight="141" prefHeight="141" />
        <RowConstraints maxHeight="141" minHeight="141" prefHeight="141" />
        <RowConstraints maxHeight="141" minHeight="141" prefHeight="141" />
        <RowConstraints maxHeight="141" minHeight="141" prefHeight="141" />
        <RowConstraints maxHeight="141" minHeight="141" prefHeight="141" />
    </rowConstraints>
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="1" GridPane.rowSpan="3">
            <children>
                <Label fx:id="confirmtitle" prefHeight="85.0" prefWidth="842.0" text="Reset Code Sent!">
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin></Label>
                <Label text="A reset code has been sent to your email address. Please enter the code below to reset your Password. " wrapText="true" />
                <HBox>
                    <children>
                        <Label fx:id="code" text="Reset Code: " />
                        <TextField>
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin></TextField>
                    </children>
                    <VBox.margin>
                        <Insets top="30.0" />
                    </VBox.margin>
                </HBox>
                <Button fx:id="changePassword" mnemonicParsing="false" onAction="#haldleChangePassword" text="Reset Password!">
                    <VBox.margin>
                        <Insets left="655.0" top="50.0" />
                    </VBox.margin></Button>
            </children>
        </VBox>
    </children>

</GridPane>
